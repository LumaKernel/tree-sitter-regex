{
  "name": "regex",
  "rules": {
    "pattern": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "disjunction"
        },
        {
          "type": "SYMBOL",
          "name": "term"
        }
      ]
    },
    "disjunction": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "term"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "|"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "term"
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              }
            ]
          }
        }
      ]
    },
    "term": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "atom"
      }
    },
    "atom": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "assertion"
        },
        {
          "type": "SYMBOL",
          "name": "pattern_character"
        },
        {
          "type": "SYMBOL",
          "name": "character_class"
        },
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "SYMBOL",
          "name": "non_capturing_group"
        }
      ]
    },
    "non_capturing_group": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "(?:"
        },
        {
          "type": "SYMBOL",
          "name": "pattern"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "assertion": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "^"
        },
        {
          "type": "STRING",
          "value": "$"
        },
        {
          "type": "STRING",
          "value": "\\b"
        },
        {
          "type": "STRING",
          "value": "\\B"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "(?"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": "="
                },
                {
                  "type": "STRING",
                  "value": "!"
                },
                {
                  "type": "STRING",
                  "value": "<="
                },
                {
                  "type": "STRING",
                  "value": "<!"
                }
              ]
            },
            {
              "type": "SYMBOL",
              "name": "pattern"
            },
            {
              "type": "STRING",
              "value": ")"
            }
          ]
        }
      ]
    },
    "character_class": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "^"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "class_range"
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "class_range": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "class_atom"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "-"
                },
                {
                  "type": "SYMBOL",
                  "name": "class_atom"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "class_atom": {
      "type": "PATTERN",
      "value": "[^\\\\\\]\\-]"
    },
    "pattern_character": {
      "type": "PATTERN",
      "value": "[^\\^\\$\\\\\\.\\*\\+\\?\\(\\)\\[\\]\\{\\}\\|]"
    },
    "quantifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "quantifier_prefix"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "?"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "quantifier_prefix": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "*"
        },
        {
          "type": "STRING",
          "value": "+"
        },
        {
          "type": "STRING",
          "value": "?"
        }
      ]
    },
    "atom_escape": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "character_class_escape"
        },
        {
          "type": "SYMBOL",
          "name": "character_escape"
        }
      ]
    },
    "character_class_escape": {
      "type": "PATTERN",
      "value": "[dDsSwW]"
    },
    "character_escape": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "control_escape"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "c"
            },
            {
              "type": "SYMBOL",
              "name": "control_letter"
            }
          ]
        }
      ]
    },
    "control_escape": {
      "type": "PATTERN",
      "value": "[fnrtv]"
    },
    "control_letter": {
      "type": "PATTERN",
      "value": "[a-zA-Z]"
    }
  },
  "extras": [],
  "conflicts": [],
  "externals": [],
  "inline": []
}